    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "14.04",
        "16.04",
        "18.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "Archlinux"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.10.0 < 7.0.0"
    }
  ],
  "tags": [
    "prometheus",
    "monitoring",
    "metrics"
  ]
}
